#!/usr/bin/env bash
DOC=$(echo -e "$HOME/thesis.bib\n$HOME/Documents/thesis/thesis.bib\n$HOME/Documents/standard.bib" | dmenu -l 10 -c)
echo $DOC
echo "\n" $(xsel -o) >> $DOC
python3 $HOME/.local/share/bin/clean_bib.py
